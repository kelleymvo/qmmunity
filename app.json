{
  "expo": {
    "name": "qmmunity",
    "slug": "qmmunity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
        "bundleIdentifier": "com.qmmunity.qmmunity",
        "config": {
          "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.979012396422-tr8cce4tpuomf7cmghiep8j8b0ireb6e"
        }
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"},
          
      "package": "com.qmmunity.qmmunity"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
